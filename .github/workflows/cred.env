# Copy to .env - fill values
# Database
DB_USER=n8n
DB_PASSWORD=change_me
DB_DATABASE=n8n

# Basic auth for n8n UI (recommended)
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=strongpassword

# Public URL - host (required for webhooks)
# Example: N8N_HOST=flows.example.com
N8N_HOST=example.com
WEBHOOK_URL=https://example.com/

# Timezone (optional)
GENERIC_TIMEZONE=UTC

# Encryption key (important for protecting credentials)
# Generate: openssl rand -hex 32
N8N_ENCRYPTION_KEY=replace_with_a_32_byte_hex_string
